[run]
source = .
omit = 
    */migrations/*
    */tests/*
    */venv/*
    manage.py
    */wsgi.py
    */asgi.py
    dzvolunteer/settings.py
    create_test_data.py
    setup_database.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
skip_covered = False
show_missing = True

[html]
directory = htmlcov
